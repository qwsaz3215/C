用遞迴寫費氏數列

#include<stdio.h> 
#include<stdlib.h>
#include"Test2.h"
int main (void)
{
	int n, r;
	printf("請輸入n值:");
	scanf("%d", &n);
	
	r = tfact(n);
	printf("%d的費氏數列為%d\n", n,r);
}


 int tfact(int i)
 {
 	if (i==1) return 0;
 	else if (i==2) return 1;
 	else return tfact(i-1) + tfact(i-2);
 }
